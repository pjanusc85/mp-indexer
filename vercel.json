{
  "functions": {
    "api/cron-indexer.js": {
      "maxDuration": 10
    },
    "api/process-blocks.js": {
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/cron-indexer",
      "schedule": "0 12 * * *"
    }
  ],
  "env": {
    "RSK_RPC_URL": "@rsk_rpc_url",
    "SUPABASE_URL": "@supabase_url", 
    "SUPABASE_ANON_KEY": "@supabase_anon_key"
  }
}